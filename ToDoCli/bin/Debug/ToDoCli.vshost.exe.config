<?xml version="1.0"?>
<configuration>
  <system.serviceModel>
    <client>
    <endpoint    address="http://localhost:8080/Item"
                 contract="ItemProxy.IItemList"
                 binding="webHttpBinding"
                 behaviorConfiguration="CliItemBehavior"
                />
    </client>
    <behaviors>
      <endpointBehaviors>
        <behavior name="CliItemBehavior">
          <webHttp />
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>

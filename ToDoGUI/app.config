<?xml version="1.0"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
  <system.serviceModel>
    <client>
      <endpoint    address="http://localhost:8000/Item"
                   contract="ToDoItemList.IItemList"
                   binding="webHttpBinding"
                   name="CliEndPoint"
                   behaviorConfiguration="CliItemBehavior"
                />
    </client>
    <behaviors>
      <endpointBehaviors>
        <behavior name="CliItemBehavior">
          <webHttp defaultOutgoingResponseFormat="Json"/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
